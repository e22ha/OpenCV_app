<Window x:Class="OpenCV2_photoapp.logWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:OpenCV2_photoapp"
        mc:Ignorable="d"
        Title="logWin" Height="450" Width="400">
    <Grid Background="#161616">
        <Grid.Resources>
            <Style TargetType="ListBox">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Foreground" Value="Azure"></Setter>
            </Style>
        </Grid.Resources>
        <ListBox Name="ListBox" IsHitTestVisible="False">
            <ListBoxItem Content="LOGGER" FontWeight="Bold" HorizontalContentAlignment="Center"></ListBoxItem>
        </ListBox>
    </Grid>
</Window>
